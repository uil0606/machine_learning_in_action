    "        fea_bill=pd.merge(fea_bill,i,how='left',on='id')\n",
    "    \n",
    "    fea_bill['bef_bill_preloan_bank_rate']=fea_bill['bef_bill_cre_bank_cnt']/fea_bill['bef_bill_preloan_bank_cnt']\n",
